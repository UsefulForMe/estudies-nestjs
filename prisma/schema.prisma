generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserAuth {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  email    String   @unique @db.String
  password String   @db.String
  roles    String[]
  isAdmin  Boolean  @default(false)
}
