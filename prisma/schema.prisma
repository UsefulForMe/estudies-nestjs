generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum UserType {
  teacher
  student
  parents
}

model UserAuth {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  email    String   @unique @db.String
  password String   @db.String
  type     UserType @default(student)
}
